{
  "name": "complay",
  "version": "0.0.40",
  "description": "Complay is a flexible JavaScript architecture library which let's us build our own framework/stack as needed.",
  "main": "complay.js",
  "files": [
    "dist",
    "helpers",
    "lib",
    "extensions",
    "generics",
    "*.js",
    "!*.config.js"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/HenriPodolski/complay.git"
  },
  "scripts": {
    "prepublish": "npm run build",
    "test": "karma start --single-run --no-auto-watch karma.config.production.js",
    "tdd": "karma start karma.config.js",
    "build": "n-clean lib && n-clean extensions && n-clean helpers && npm shrinkwrap --dev && npm run build-all",
    "build-all": "npm run build-babel && npm run build-prebuild",
    "build-babel": "npm run build-babel-complay && npm run build-babel-config && npm run build-babel-lib && npm run build-babel-helpers && npm run build-babel-extensions && npm run build-babel-generics",
    "build-babel-complay": "babel src/complay.js -s --out-file ./complay.js",
    "build-babel-config": "babel src/default-config.js -s --out-file ./default-config.js",
    "build-babel-lib": "babel src/lib -s --extensions .js --out-dir lib",
    "build-babel-helpers": "babel src/helpers -s --extensions .js --out-dir helpers",
    "build-babel-extensions": "babel src/extensions -s --extensions .js --out-dir extensions",
    "build-babel-generics": "babel src/generics -s --extensions .js --out-dir generics",
    "build-prebuild": "webpack --config webpack.config.production.js",
    "lint": "eslint ."
  },
  "author": "Henri Podolski",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.8.0",
    "babel-core": "^6.7.7",
    "babel-eslint": "^6.0.4",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-es2015-classes": "^6.9.0",
    "babel-plugin-transform-es2015-modules-umd": "^6.8.0",
    "babel-polyfill": "^6.9.0",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "chai": "^3.5.0",
    "eslint": "^2.9.0",
    "eslint-plugin-react": "^5.0.1",
    "istanbul-instrumenter-loader": "^0.2.0",
    "karma": "^0.13.22",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^0.2.2",
    "karma-coverage": "^0.5.5",
    "karma-firefox-launcher": "^0.1.7",
    "karma-ie-launcher": "^0.2.0",
    "karma-mocha": "^0.2.2",
    "karma-mocha-reporter": "^2.0.0",
    "karma-phantomjs-launcher": "^1.0.0",
    "karma-sinon": "^1.0.5",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-spec-reporter": "0.0.26",
    "karma-webpack": "^1.7.0",
    "mocha": "^2.4.5",
    "npm-build-tools": "^2.2.5",
    "phantomjs-prebuilt": "^2.1.5",
    "react-dom": "^15.2.1",
    "react-hot-loader": "^1.3.0",
    "sinon": "git+https://github.com/uberVU/Sinon.JS",
    "webpack": "^1.13.0"
  },
  "dependencies": {
    "handlebars": "^4.0.5",
    "plite": "0.0.3",
    "react": "^15.2.1",
    "react-dom": "^15.2.1"
  }
}
